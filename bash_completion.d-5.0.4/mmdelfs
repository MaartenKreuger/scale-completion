_mmdelfs_completion()
{
  COMPREPLY=()
  case "${#COMP_WORDS[@]}" in
    2) COMPREPLY=($(compgen -W "$(_localfs_names)" -- "${COMP_WORDS[1]}"));;
    *) COMPREPLY=($(compgen -W '-p' -- "${COMP_WORDS[COMP_CWORD]}"));;
  esac
}
complete -F _mmdelfs_completion mmdelfs
