_mmafmlocal_completion()
{
  COMPREPLY=()
  case "${#COMP_WORDS[@]}" in
    2) COMPREPLY=($(compgen -W "ls stat" -- "${COMP_WORDS[1]}"));;
    *) case ${COMP_WORDS[1]} in
         ls|stat) COMPREPLY=($(compgen -f -o filenames -- "${COMP_WORDS[COMP_CWORD]}"));;
       esac;;
  esac
}
complete -F _mmafmlocal_completion mmafmlocal
