_mmquotaon_completion()
{
  COMPREPLY=($(compgen -W '-u -g -j -v -a $(_localfs_names)' -- "${COMP_WORDS[COMP_CWORD]}"))
}
complete -F _mmquotaon_completion mmquotaon
