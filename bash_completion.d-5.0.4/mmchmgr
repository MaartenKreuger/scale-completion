_mmchmgr_completion()
{
  COMPREPLY=()
  case "${#COMP_WORDS[@]}" in
    2) COMPREPLY=($(compgen -W "$(_localfs_names) -c" -- "${COMP_WORDS[1]}"));;
    3) COMPREPLY=($(compgen -W "$(_manager_nodes)" -- "${COMP_WORDS[COMP_CWORD]}"));;
  esac
}
complete -F _mmchmgr_completion mmchmgr
