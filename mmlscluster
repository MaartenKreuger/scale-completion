_mmlscluster_completion()
{
  COMPREPLY=($(compgen -W '-Y --cnfs --ces --cloud-gateway' -- "${COMP_WORDS[COMP_CWORD]}"))
}
complete -F _mmlscluster_completion mmlscluster
