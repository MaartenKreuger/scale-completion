_mmdelnodeclass_completion()
{
  COMPREPLY=()
  case "${#COMP_WORDS[@]}" in
    2) COMPREPLY=($(compgen -W "$(_user_nodeclasses)" -- "${COMP_WORDS[COMP_CWORD]}"));;
  esac
}
complete -F _mmdelnodeclass_completion mmdelnodeclass
